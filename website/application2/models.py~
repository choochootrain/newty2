from django.db import models

class Model1(models.Model):
    xstreet0 = models.CharField(max_length=50, blank=True, null=True)
    xstreet1 = models.CharField(max_length=50, blank=True, null=True)
    city = models.CharField(max_length=50, blank=True, null=True)
    region = models.CharField(max_length=50, blank=True, null=True)
    def __unicode__(self):
        return self.xstreet0 + " at " + self.xstreet1 + " " + self.city + " " + self.region

class Model2(models.Model):
    num_beds = models.IntegerField(blank=True, null=True)
    num_baths = models.IntegerField(blank=True, null=True)
    square_ft = models.IntegerField(blank=True, null=True)
    full_address = models.BooleanField()
    address_obj = models.ForeignKey(Address)
    def __unicode__(self):
        return "Apt at: " + unicode(self.address_obj)

class Model3(models.Model):
    post_title = models.CharField(max_length=75)
    post_time = models.CharField(max_length=30)
    price = models.IntegerField(blank=True, null=True)
    post_id = models.CharField(max_length=25)
    apartment_obj = models.ForeignKey(Apartment)
    access_time = models.DateTimeField()
    def __unicode__(self):
        return self.post_title
