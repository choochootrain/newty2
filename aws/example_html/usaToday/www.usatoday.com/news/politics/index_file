<script type="text/javascript">
var pagePath, pagePathParts;

jQuery(document).ready(function(){
// highlight the current navigation item
var url = window.location.href;
if (url.indexOf("politics/") != -1) {
pagePath = url.substring(url.indexOf("news/politics/"), url.length);
pagePathParts = pagePath.split("/");
if (pagePathParts.length > 3)
pagePath = pagePathParts[0] + "/" + pagePathParts[1] + "/" + pagePathParts[2];
jQuery("#election-nav li a").not("#election-sub a").each(function(){
if (jQuery(this).attr("href").indexOf(pagePath) != -1) {
jQuery(this).addClass("active");
return;
}
});
}
});

</script>
