function writeExpandedBanner(value)
{
if(value != null && value != '') {         
jQuery("#usat_PosterBlog").html('' + value + '');  
if(kk == 0) {
jQuery('.preroll-ad-wrapper').slideToggle('slow');
kk=1;
}
}
else { 
if(kk == 1) {
jQuery('.preroll-ad-wrapper').slideToggle('slow');
kk=0;
}
}    
}


</script>
