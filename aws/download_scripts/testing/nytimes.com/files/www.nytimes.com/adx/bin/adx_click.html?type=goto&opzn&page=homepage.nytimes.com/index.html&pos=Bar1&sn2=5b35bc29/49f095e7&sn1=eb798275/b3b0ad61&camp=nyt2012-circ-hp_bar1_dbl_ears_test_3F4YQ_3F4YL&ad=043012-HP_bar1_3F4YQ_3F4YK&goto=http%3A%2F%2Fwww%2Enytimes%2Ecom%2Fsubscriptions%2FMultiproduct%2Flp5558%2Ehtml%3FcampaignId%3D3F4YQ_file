<!--Code for WTO A/B Testing  -->
<span id="wto_script" style="display: none;"></span>

<script type="text/javascript">

(function() {
    NYTD.wto.testAlias = "ta_landingPage_multiproduct";
    NYTD.wto.contentLoaded = false;

    var tryNow = function() {
        if(document.body && window.WTOptimize && NYTD.wto.libraryLoaded) {
            setTimeout(function() {
                WTOptimize.setup(
                    NYTD.wto.testAlias, {
                        s_urlParts: true,
                        s_pageTimeout: '30000'
                    });
                document.body.style.visibility='visible';
            }, 10);
        }
        else {
            setTimeout(tryNow, 10);
        }
    };
    
    setTimeout(tryNow, 10);

    var handleInputClick = function(e) {
        var target;
        if(e.target) {target = e.target;}
        else { target = e.srcElement; }
        
        if(target.tagName === 'INPUT') { 
            NYTD.wto.nonLinkEventConversion({'e':e});
        }
        if(target.id === 'subscribe')     { NYTD.wto.submitHandler('subscribe', goToURL); }
        if(target.id === 'btn_unlimited') { NYTD.wto.submitHandler('subscribe', goToURL); }

        if(NYTD.wto.firstClick) {NYTD.wto.setFirstClick(); }
        else { return; }
    };

    NYTD.wto.setConversionPoints = function(experimentName) {
        if(NYTD.wto.testRunning) { 
            $("#var1_Dig, #var2_Dig, #var3_Dig, #var3_Edu").click(function() { 
                NYTD.wto.setOrderFormCookie(NYTD.wto.testAlias + ":qs:1", 1); 
            });
            $("#paymodel").click(handleInputClick);
            $('a').bind("mouseup", function(e) { 
                NYTD.wto.linkConversion({'e':e});
            });
        }
        else {
            $("#subscribe").click(goToURL);
        }
    };

    NYTD.wto.showContent = function(experimentName) {
        // get rid of old orderForm cookie
        NYTD.wto.setOrderFormCookie("", -1);
        if(experimentName !== "control") {
            $("#giftSubscriptions").hide();
            var $abTestVariations = $("#abTest_variations");
            $abTestVariations.show();
            var whichVariation = {var1:"largeImages", var2:"largeIcons", var3:"smallIcons"}[experimentName];
            if(whichVariation) { $abTestVariations.find("#" + whichVariation).show(); }
        }
        NYTD.wto.contentLoaded = true;
        NYTD.wto.setConversionPoints(experimentName); 
        
    };

  // Display default markup in the event that Webtrends does not load
    setTimeout(function() {
        if(!NYTD.wto.contentLoaded) { 
            NYTD.wto.log("wto not running, showing default content");
            NYTD.wto.showContent('control'); 
        }
    }, 3000);
    
})();
</script>


<!--End of code for WTO A/B Testing  -->



