	// Video callbacks
	function video_initCallback(carousel) {
		jQuery('#video_section div.counter_mark').bind('click',function() {
			carousel.scroll(jQuery.jcarousel.intval(jQuery(this).index() * 3 + 1));
			return false;
		});
	};
	function video_itemFirstInCallback(carousel, item, idx, state) {
		idx = --idx / 3;
		jQuery('#video_section div.counter_mark').eq(idx).addClass('active').siblings().removeClass('active');
	};

	// Episodes callbacks
	function episode_initCallback(carousel) {
		jQuery('#ep_next').bind('click',function() {
			carousel.scroll(1);
			jQuery(this).addClass('active');
			jQuery(this).siblings().removeClass('active');
			//return false;
		});
		jQuery('#ep_prev').bind('click',function() {
			carousel.scroll(2);
			jQuery(this).addClass('active');
			jQuery(this).siblings().removeClass('active');
			//return false;
		});
		return false;
	};

	// Meet callbacks
	function meet_initCallback(carousel) {
		jQuery('#meet-pod div.counter_mark').bind('click',function() {
			carousel.scroll(jQuery.jcarousel.intval((jQuery(this).index() - 1) * 2 + 1));
			return false;
		});
	};
	function meet_itemFirstInCallback(carousel, item, idx, state) {
		jQuery('#meet_next').unbind();
		jQuery('#meet_prev').unbind();
		var next_idx = idx + 2;
		var prev_idx = idx - 2;
		//alert('Next: ' + next_idx + ' - Prev: ' + prev_idx);
		jQuery('#meet_next').bind('click',function() {carousel.scroll(next_idx);});
		jQuery('#meet_prev').bind('click',function() {carousel.scroll(prev_idx);});
		idx = --idx / 2;
		jQuery('#meet-pod div.counter_mark').eq(idx).addClass('active').siblings().removeClass('active');
	};
	
	// Photo Gallery callbacks
	function photos_initCallback(carousel) {
		jQuery('#photo-gallery-pod div.counter_mark').bind('click',function() {
			//carousel.scroll(jQuery.jcarousel.intval((jQuery(this).index() -1) * 4 + 1));
			carousel.scroll(jQuery.jcarousel.intval(jQuery(this).index()));
			//alert(jQuery(this).index());
			return false;
		});
	};
	function photos_itemFirstInCallback(carousel, item, idx, state) {
		jQuery('#photos_next').unbind();
		jQuery('#photos_prev').unbind();
		var next_idx = idx + 1;
		var prev_idx = idx - 1;
		jQuery('#photos_next').bind('click',function() {carousel.scroll(next_idx);});
		jQuery('#photos_prev').bind('click',function() {carousel.scroll(prev_idx);});
		//idx = --idx / 4;
		idx = --idx;
		jQuery('#photo-gallery-pod div.counter_mark').eq(idx).addClass('active').siblings().removeClass('active');
	};
	
