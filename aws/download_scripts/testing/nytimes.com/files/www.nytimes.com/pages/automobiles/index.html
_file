<script type='text/javascript'>
(function() {
    if (NYTD && NYTD.Video && NYTD.Video.Factory) {
        NYTD.Video.Factory.loadDependencies(function() {
            var config = {
                container: 'video-nyt-classifieds'
                    ,bigAdContainer: 'video-nyt-classifieds-adContainer'
                    ,id: 'classifieds_videos_' + new
Date().getTime().toString() 
                    ,playerId: '760102458001'
                    ,videoId: '100000001395090'
                    ,publisherId: '1749339200'
                    ,width: 300 
                    ,height: 250 
                    ,autoStart: true 
                    ,autoRender: true
                    ,playerType: 'classifeds|automobiles'
                    ,bgcolor: '#000000' 
                    ,quality: 'high'
                    ,videoIds: [100000001108352, 100000000895665,
100000001487768, 100000001472533, 100000001229388]
                    //,playlistId: '1194811622349'
                    ,autoPlayPlaylist: true
                    ,isLive: true
                    ,connectOnLoad:'true'
                    ,enableInitialBandwidthDetection: 'true'
                    ,pollService: ''
                    ,enableLivePoll: false
                    ,adxAdPositions: 'VideoPlayerAd, VideoBigAd,ADX_CLIENTSIDE'
                    ,adxPagename:
'www.nytimes.com/pages/automobiles/index.html'
            };

            if (typeof NYTD.Video.Factory.create === 'function') {
                var video = NYTD.Video.Factory.create(config);

                if (video) {
                    var menuModule = null;
                    var sub = video.getSubscription();
                    var skipAd = false;
                    var plays = 0;

                    var onTemplateReady = function(e) {
               video.mute();
                        menuModule = video.getMenuModule();
                    };

                    var onStreamStart = function(e) {
                        var mod = (++plays % 2);
                        if (mod === 0) {
                            video.getAdModule().enableExternalAds(true);
                        } else {
                            video.getAdModule().enableExternalAds(false);
                        }

                        if (menuModule.isMenuPageShowing()) {
                            menuModule.closeMenuPage();
                        }
                    };

                    if (sub) {
                        sub.subscribe(NYTD.Video.VideoEvents.TEMPLATE_READY,
onTemplateReady);
                        sub.subscribe(NYTD.Video.VideoEvents.STREAM_START,
onStreamStart);
                    }
                }
            }
        });
    }
})();
</script>
