			<script type="text/javascript">
				function tc_loadMoreEvents(timeperiod) {
					displayedEvents = jQuery('.event.'+timeperiod);
					jQuery('#load-more-'+timeperiod+' a').html('Loading....');
					jQuery.post( tc_ajax.ajaxurl, {
						action: 'tc_load_more_events',
						offset: displayedEvents.length,
						timeperiod: timeperiod
					}, function(response){
						if( '' == response ) {
							jQuery('#load-more-'+timeperiod).html('<span>No more '+timeperiod+' events</span>');
						} else {
							displayedEvents.last().after('<p>'+response+'</p>').show();
							jQuery('#load-more-'+timeperiod+' a').html('View more '+timeperiod+' events');
						}
					});
	
					return false;
				};
			</script>

