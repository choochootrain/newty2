			if(navigator.userAgent.match(/iPad/i)) {				
				var num=1 + Math.floor(Math.random()*11);
				var adNode = "<div id='aol_ipad_banner' style='width:1008px;margin:0px auto;height:45px;'>";
				// 50-50 weight
				if(num >= 1 && num <= 5) {
					adNode += "<a href='http://t.aol.com?ncid=bannadnew00000354'><img src='http://tctechcrunch2011.files.wordpress.com/2012/02/aol-ipadapp-1.jpg' /></a>";
				} else {
					adNode += "<a href='http://t.aol.com?ncid=bannadnew00000358'><img src='http://tctechcrunch2011.files.wordpress.com/2012/02/aoli-ipadapp-2.jpg' /></a";
				}
				adNode += "</div>";
				$(adNode).insertBefore("#top");
		
				if($('#aol_ipad_banner').length > 0) {
					$(window).scroll(function() {
						if($(window).scrollTop() <= $('#aol_ipad_banner').height()) {
							$('#aol_ipad_banner').show();
							$('#site-logo-small').hide();
						} else {
							$('#aol_ipad_banner').hide();
							$('#site-logo-small').show();
						}
					});
				}
			}
		});
