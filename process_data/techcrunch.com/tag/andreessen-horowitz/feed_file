		<media:content url="http://0.gravatar.com/avatar/424afc156b83d8f4ba90ec5fdf6f8f11?s=96&#38;d=identicon&#38;r=G" medium="image">
			<media:title type="html">sarah-lacy</media:title>
		</media:content>

