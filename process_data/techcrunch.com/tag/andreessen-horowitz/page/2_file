			// Load images on DOM ready -- all in one batched call
			// This could prevent the stutter on scrolling
			$.each($("img[data-src]"), function() {
                var img = this, 
					$img = $(img);
			            
                img.src = $img.attr( "data-src" );
			            
            });

			//
			// PAGE-SPECIFIC SCRIPT HERE
			//
			techcrunch.page.Home.getInstance();
			techcrunch.util.VideoPlayerManager.getInstance();
			new techcrunch.module.Header("#module-header");
			new techcrunch.module.GotATip();
