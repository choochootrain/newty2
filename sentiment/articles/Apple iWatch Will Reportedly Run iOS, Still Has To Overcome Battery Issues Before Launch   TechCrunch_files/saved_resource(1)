e.getAttribute(DEFAULT_ARTICLE_ID_ATTRIBUTE)};module.defaults.get_article_id_legacy=function(e){return e.getAttribute(o)};module.defaults.get_site_id_from_script_src=module.helpers.memoize(function(e){var t=module.helpers.get_script_element();if(typeof t==="undefined")return null;var n=t.src.match("#bn=([^\\&]*)");if(n&&n.length>=2)return n[1];return null});module.defaults.get_site_id_from_attribute=function(e){return e.getAttribute(DEFAULT_SITE_ID_ATTRIBUTE)};module.defaults.get_articles=function(){var e=module.defaults.find_elements(),t,n={},r,i,s;for(var o=0,u=e.length;o<u;o++){t=e[o];r=module.defaults.get_site_id_from_attribute(t)||module.defaults.get_site_id_from_script_src(t);if(!(r in n))n[r]={};i=n[r];s=module.defaults.get_article_id(t);if(!(s in i))i[s]={elements:[]};i[s].elements.push(t)}return n};module.defaults.get_articles_legacy=function(){var e=module.defaults.find_elements(LEGACY_CLASS_NAME),t,n={},r,i,s;for(var o=0,u=e.length;o<u;o++){t=e[o];r=module.defaults.get_site_id_from_script_src(t);if(!(r in n))n[r]={};i=n[r];s=module.defaults.get_article_id_legacy(t);if(!(s in i))i[s]={elements:[]};i[s].elements.push(t)}return n};module.defaults.replace_all=module.helpers.make_replacer(DEFAULT_REPLACER_REGEX);module.fetch=function(e,t,n){var i=Array.prototype.slice.call(arguments),o=0;module.loaded=true;if(i.length===1){t=i[0];e=module.defaults.get_articles()}if(typeof e==="function"){e=e()}if(!module.helpers.isArray(e)){e=module.helpers.articles_object_to_list(e)}var u="http://bootstrap.{domain}{path}",a=module.helpers.hash_articles(e);if(a==null){if(typeof n==="function"){n()}return}u=u.replace("{domain}",module.helpers.get_domain()||r).replace("{path}",s).replace("{hash}",a);var f=function(n){o=o+1;if(n.status==="ok"){var r=module.helpers.articles_list_to_object(e),n=module.helpers.ensure_correct_articles_object(n.data),i=module.helpers.merge_articles_objects(n,r);t.call(null,i)}else if(n.code==503&&o<MAX_FETCH_ATTEMPTS){setTimeout(function(){module.help