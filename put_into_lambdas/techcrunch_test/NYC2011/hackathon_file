				var adNode = "<div id='aol_ipad_banner' style='width:1008px;margin:0px auto;height:45px;'>";
				// 50-50 weight
				if(num >= 1 && num <= 5) {
					adNode += "<a href='http://t.aol.com?ncid=bannadnew00000354'><img src='http://tctechcrunch2011.files.wordpress.com/2012/02/aol-ipadapp-1.jpg' /></a>";
